# Anthropic API Key (Required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Node Environment
NODE_ENV=development

# MCP Server Logging
LOG_LEVEL=info

# Cache Configuration (Optional)
CACHE_TTL_SEARCH=3600000      # 1 hour in milliseconds
CACHE_TTL_PRODUCT=86400000     # 24 hours in milliseconds
CACHE_TTL_CITATIONS=21600000   # 6 hours in milliseconds
